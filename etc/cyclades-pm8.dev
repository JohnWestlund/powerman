# Cyclades PM8i s/w v. 1.0.9a
#
specification "pm8" {
	timeout 	10

	plug name { "1" "2" "3" "4" "5" "6" "7" "8" }

	script login {
		send "exit\r\r"
		expect "Username:"
		send "admin\r"
		expect "Password:"
		send "pm8\r"
		expect "pm>"
	}
	script logout {
		send "exit\r"
	}
	script status_all {
		send "status 1-8\r"
		foreachplug {
			expect " ([1-8]+).*(ON|OFF)"
			setplugstate $1 $2 on="ON" off="OFF"
		}
		expect "pm>"
	}
	script on {
		send "on %s\r"
		expect "pm>"
	}
	script on_all {
		send "on 1-8\r"
		expect "pm>"
	}
	script off {
		send "off %s\r"
		expect "pm>"
	}
	script off_all {
		send "off 1-8\r"
		expect "pm>"
	}
	script cycle {
		send "off %s\r"
		expect "pm>"
		delay 2
		send "on %s\r"
		expect "pm>"
	}
	script cycle_all {
		send "off 1-8\r"
		expect "pm>"
		delay 2
		send "on 1-8\r"
		expect "pm>"
	}
}
