specification {
	name 		"vicebox"
	type 		"TCP"
	offstring 	"0"
	onstring  	"1"
	plugcount 	10
	timeout 	100.0
	script login {
		expect "V2\.2\r\n"
		send "auth password\r\n"
		expect "OK\r\n"
	}
	script logout {
		send "q\r\n"
	}
	script status_all {
		send "ps *\r\n"
		expect "N1:([01]) N2:([01]) N3:([01]) N4:([01]) N5:([01]) N6:([01]) N7:([01]) N8:([01]) N9:([01]) N10:([01])[[:space:]]*\r\n"
			map $1 "1"
			map $2 "2"
			map $3 "3"
			map $4 "4"
			map $5 "5"
			map $6 "6"
			map $7 "7"
			map $8 "8"
			map $9 "9"
			map $10 "10"
	}
	script status_soft_all {
		send "ns *\r\n"
		expect "N1:([01]) N2:([01]) N3:([01]) N4:([01]) N5:([01]) N6:([01]) N7:([01]) N8:([01]) N9:([01]) N10:([01])[[:space:]]*\r\n"
			map $1 "1"
			map $2 "2"
			map $3 "3"
			map $4 "4"
			map $5 "5"
			map $6 "6"
			map $7 "7"
			map $8 "8"
			map $9 "9"
			map $10 "10"
	}
	script on {
		send "ph %s\r\n"
		expect "OK\r\n"
	}
	script on_all {
		send "ph *\r\n"
		expect "OK\r\n"
	}
	script off {
		send "pl %s\r\n"
		expect "OK\r\n"
	}
	script off_all {
		send "pl %s\r\n"
		expect "OK\r\n"
	}
	script cycle {
		send "pl %s\r\n"
		expect "OK\r\n"
		send "ph %s\r\n"
		expect "OK\r\n"
	}
	script cycle_off {
		send "pl *\r\n"
		expect "OK\r\n"
		send "ph *\r\n"
		expect "OK\r\n"
	}
	script reset {
		send "rp %s\r\n"
		expect "OK\r\n"
	}
	script reset_all {
		send "rp *\r\n"
		expect "OK\r\n"
	}
	plug name "1"
	plug name "2"
	plug name "3"
	plug name "4"
	plug name "5"
	plug name "6"
	plug name "7"
	plug name "8"
	plug name "9"
	plug name "10"
}
