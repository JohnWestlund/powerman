specification {
	name 		"vicebox"
	type 		"TCP"
	offstring 	"0"
	onstring  	"1"
	maxplugcount 	10
	timeout 	100.0
	script login {
		expect "V2\.2\r\n"
		send "auth password\r\n"
		expect "OK\r\n"
	}
	script logout {
		send "q\r\n"
	}
	script status_all {
		send "ps *\r\n"
		expect "N1:([01]) N2:([01]) N3:([01]) N4:([01]) N5:([01]) N6:([01]) N7:([01]) N8:([01]) N9:([01]) N10:([01])[[:space:]]*\r\n"
		setstatus "1" $1
		setstatus "2" $2
		setstatus "3" $3
		setstatus "4" $4
		setstatus "5" $5
		setstatus "6" $6
		setstatus "7" $7
		setstatus "8" $8
		setstatus "9" $9
		setstatus "10" $10
	}
	script status_soft_all {
		send "ns *\r\n"
		expect "N1:([01]) N2:([01]) N3:([01]) N4:([01]) N5:([01]) N6:([01]) N7:([01]) N8:([01]) N9:([01]) N10:([01])[[:space:]]*\r\n"
		setstatus "1" $1
		setstatus "2" $2
		setstatus "3" $3
		setstatus "4" $4
		setstatus "5" $5
		setstatus "6" $6
		setstatus "7" $7
		setstatus "8" $8
		setstatus "9" $9
		setstatus "10" $10
	}
	script on {
		send "ph %s\r\n"
		expect "OK\r\n"
	}
	script on_all {
		send "ph *\r\n"
		expect "OK\r\n"
	}
	script off {
		send "pl %s\r\n"
		expect "OK\r\n"
	}
	script off_all {
		send "pl %s\r\n"
		expect "OK\r\n"
	}
	script cycle {
		send "pl %s\r\n"
		expect "OK\r\n"
		send "ph %s\r\n"
		expect "OK\r\n"
	}
	script cycle_off {
		send "pl *\r\n"
		expect "OK\r\n"
		send "ph *\r\n"
		expect "OK\r\n"
	}
	script reset {
		send "rp %s\r\n"
		expect "OK\r\n"
	}
	script reset_all {
		send "rp *\r\n"
		expect "OK\r\n"
	}
	plug name "1"
	plug name "2"
	plug name "3"
	plug name "4"
	plug name "5"
	plug name "6"
	plug name "7"
	plug name "8"
	plug name "9"
	plug name "10"
}
